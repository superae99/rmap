# Testing Client Environment Variables (Example)

# API Configuration
VITE_API_URL=http://localhost:8080/api
VITE_API_TIMEOUT=30000

# Kakao Map Configuration
VITE_KAKAO_API_KEY=your_test_kakao_api_key

# Environment
VITE_ENV=testing

# Debug Mode
VITE_DEBUG=false
VITE_SHOW_CONSOLE_LOGS=false

# Feature Flags
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_PANEL=false
VITE_ENABLE_TEST_BANNER=true