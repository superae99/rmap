# Production Client Environment Variables

# API Configuration
VITE_API_URL=https://api.your-domain.com/api
VITE_API_TIMEOUT=30000

# Kakao Map Configuration
VITE_KAKAO_API_KEY=YOUR_PRODUCTION_KAKAO_API_KEY

# Environment
VITE_ENV=production

# Debug Mode
VITE_DEBUG=false
VITE_SHOW_CONSOLE_LOGS=false

# Feature Flags
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_PANEL=false
VITE_ENABLE_ANALYTICS=true

# Security
VITE_CSP_REPORT_URI=https://your-domain.com/csp-report